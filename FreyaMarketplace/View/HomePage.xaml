<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FreyaMarketplace.View.HomePage"
             Title="Home">

    <VerticalStackLayout Padding="20">
        <Label Text="Welcome to FreyaDev!" FontSize="24" HorizontalOptions="Center"/>
        <Label Text="This is your home page. Browse and explore." FontSize="16" HorizontalOptions="Center"/>

        <Button Text="Go to Listings" Margin="10" Clicked="OnListingsClicked"
HorizontalOptions="Center" />
        <Button Text="Create New Listing" Margin="10" Clicked="OnNewListingClicked"
HorizontalOptions="Center" />
    </VerticalStackLayout>

</ContentPage>
